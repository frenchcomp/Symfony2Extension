{
    "name": "behat/symfony2-extension",
    "type": "behat-extension",
    "description": "Symfony2 framework extension for Behat",
    "keywords": ["symfony", "framework", "bdd"],
    "homepage": "http://behat.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Konstantin Kudryashov",
            "email": "ever.zet@gmail.com"
        },
        {
            "name": "Christophe Coevoet",
            "email": "stof@notk.org"
        }
    ],
    "require": {
        "php": ">=7.2",
        "behat/behat": "^3.4.3",
        "symfony/config": "~3.4||~4.4||~5.1",
        "symfony/dependency-injection": "~3.4||~4.4||~5.1",
        "symfony/event-dispatcher": "~3.4||~4.4||~5.1",
        "symfony/http-kernel": "~3.4||~4.4||~5.1",
        "symfony/process": "~3.4||~4.4||~5.1"
    },
    "require-dev": {
        "behat/mink-extension": "~2.0",
        "behat/mink": "~1.7",
        "behat/mink-browserkit-driver": "~1.3",
        "doctrine/annotations": "~1.0",
        "phpspec/phpspec": "~6.0",
        "phpunit/phpunit": "~8.0",
        "symfony/framework-bundle": "~3.4||~4.4||~5.1",
        "symfony/form": "~3.4||~4.4||~5.1",
        "symfony/validator": "~3.4||~4.4||~5.1"
    },
    "autoload": {
        "psr-4": {
            "Behat\\Symfony2Extension\\": "src/Behat/Symfony2Extension/"
        }
    }
}
